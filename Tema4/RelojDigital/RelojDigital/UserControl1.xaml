             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RelojDigital"
             mc:Ignorable="d" Width="156" Height="337">
    <Grid HorizontalAlignment="Left" Width="156" Height="327" VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition/>
