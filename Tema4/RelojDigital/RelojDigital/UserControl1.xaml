        <Rectangle x:Name="paloAI" HorizontalAlignment="Left" Height="92" Margin="1,29,0,0" VerticalAlignment="Top" Width="22" Fill="#FFE9DBDB"/>
        <Rectangle x:Name="paloAD" HorizontalAlignment="Left" Height="92" Margin="130,29,0,0" VerticalAlignment="Top" Width="22" Fill="#FFE9DBDB"/>
        <Rectangle x:Name="paloBI" HorizontalAlignment="Left" Height="92" Margin="1,148,0,0" VerticalAlignment="Top" Width="22" Fill="#FFE9DBDB"/>
        <Rectangle x:Name="paloBD" HorizontalAlignment="Left" Height="92" Margin="130,148,0,0" VerticalAlignment="Top" Width="22" Fill="#FFE9DBDB"/>
        <Rectangle x:Name="paloC" HorizontalAlignment="Left" Height="104" Margin="66,82,0,0" VerticalAlignment="Top" Width="22" Fill="#FFE9DBDB" RenderTransformOrigin="0.5,0.5">
