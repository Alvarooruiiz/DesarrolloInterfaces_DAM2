            </Rectangle.RenderTransform>
        </Rectangle>
        <Rectangle x:Name="paloA" HorizontalAlignment="Left" Height="102" Margin="66,-34,0,0" VerticalAlignment="Top" Width="22" Fill="#FFE9DBDB" RenderTransformOrigin="0.5,0.5">
